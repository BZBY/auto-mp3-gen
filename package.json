{
  "name": "auto-mp3-gen",
  "version": "1.0.0",
  "description": "基于字幕的动漫角色对话提取系统",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "server": "cd backend && python app.py",
    "client": "cd frontend && yarn start",
    "build": "cd frontend && yarn build",
    "install-all": "cd backend && pip install -r requirements.txt && cd ../frontend && yarn install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "anime",
    "voice",
    "extraction",
    "subtitle",
    "uvr5"
  ],
  "author": "auto-mp3-gen",
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}


